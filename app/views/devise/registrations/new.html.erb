
<%= javascript_include_tag "fecha" %>
<h2>¿Eres Nuevo? Regístrate</h2>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: 'form-horizontal'}) do |f| %>
<%= devise_error_messages! %>

<div class="form-group">
    <%= f.label :nombre_1, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :nombre1, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :nombre_2, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :nombre2, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :apellido1, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :apellido1, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :apellido2, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :apellido2, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :cedula, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :cedula, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :tarjeta_profesional, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :tarjeta_profesional, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :estado_civil, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :estado_civil, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :profesion, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :profesion, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :direccion, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :direccion, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :fecha_nacimiento, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :fecha_nacimiento, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :celular, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :celular, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :telefono, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :telefono, class: "form-control",autofocus: true %>
    </div>
</div>

<div class="form-group">
    <%= f.label :perfil, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.text_field :perfil, class: "form-control",autofocus: true %>
    </div>
</div>


<div class="form-group">
    <%= f.label :email, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.email_field :email, class: "form-control" %>
    </div>
</div>

<div class="form-group">
    <%= f.label :password, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
    </div>
</div>

<div class="form-group">
    <%= f.label :password_confirmation, class: "col-sm-2 control-label" %>
    <div class="col-sm-6">
        <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control", :placeholder => "placeholder text" %>
    </div>
</div>

<div class="form-group">
    <%= f.label :Sube_Tu_Imagen, class: 'col-sm-2 control-label'  %>
    <div class="col-sm-6">
        <%= f.file_field :avatar %>
    </div>
</div>

<div class="form-group">
    <div class="col-sm-offset-2 col-sm-6">
        <%= f.submit "Registrarse", class: "btn btn-primary" %>
    </div>
</div>


<% end %>