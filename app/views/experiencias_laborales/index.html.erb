<%- model_class = ExperienciaLaboral -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:empresa) %></th>
      <th><%= model_class.human_attribute_name(:cargo) %></th>
      <th><%= model_class.human_attribute_name(:telefono) %></th>
      <th><%= model_class.human_attribute_name(:fecha_incio) %></th>
      <th><%= model_class.human_attribute_name(:fecha_terminacion) %></th>
      <th><%= model_class.human_attribute_name(:usuario_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @experiencias_laborales.each do |experiencia_laboral| %>
      <tr>
        <td><%= link_to experiencia_laboral.id, experiencia_laboral_path(experiencia_laboral) %></td>
        <td><%= experiencia_laboral.empresa %></td>
        <td><%= experiencia_laboral.cargo %></td>
        <td><%= experiencia_laboral.telefono %></td>
        <td><%= experiencia_laboral.fecha_incio %></td>
        <td><%= experiencia_laboral.fecha_terminacion %></td>
        <td><%= experiencia_laboral.usuario_id %></td>
        <td><%=l experiencia_laboral.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_experiencia_laboral_path(experiencia_laboral), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      experiencia_laboral_path(experiencia_laboral),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_experiencia_laboral_path,
            :class => 'btn btn-primary' %>
