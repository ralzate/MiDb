<%- model_class = ProfesionesUsuario -%>
<button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="$.lazybox.close()">&times;</button>
<div class="page-header">
  <h2>Profesiones Usuarios:<%= @profesiones_usuario.id %></h2>
  <table class="table table-bordered table-hover" >
    <thead>
      <tr class="info">
        <th>Usuario</th>
        <th>Profesion</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><p><%= @profesiones_usuario.usuario_id %></p></td>
        <td><p><%= @profesiones_usuario.profesion_id %></p></td> 
      </tr>
    </tbody>
  </table>
  <div class="modal-footer">
    <a href="#" class="btn btn-primary" onclick="$.lazybox.close()">Cerrar</a>
  </div>
</div>