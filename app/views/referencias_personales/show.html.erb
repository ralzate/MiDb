<%- model_class = ReferenciasPersonale -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @referencias_personale.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono) %>:</strong></dt>
  <dd><%= @referencias_personale.telefono %></dd>
  <dt><strong><%= model_class.human_attribute_name(:profesion) %>:</strong></dt>
  <dd><%= @referencias_personale.profesion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:usuario_id) %>:</strong></dt>
  <dd><%= @referencias_personale.usuario_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              referencias_personales_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_referencias_personale_path(@referencias_personale), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              referencias_personale_path(@referencias_personale),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
