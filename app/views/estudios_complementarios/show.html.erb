<%- model_class = EstudiosComplementario -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @estudios_complementario.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:entidad) %>:</strong></dt>
  <dd><%= @estudios_complementario.entidad %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fecha_terminacion) %>:</strong></dt>
  <dd><%= @estudios_complementario.fecha_terminacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:intensidad_horaria) %>:</strong></dt>
  <dd><%= @estudios_complementario.intensidad_horaria %></dd>
  <dt><strong><%= model_class.human_attribute_name(:usuario_id) %>:</strong></dt>
  <dd><%= @estudios_complementario.usuario_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              estudios_complementarios_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_estudios_complementario_path(@estudios_complementario), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              estudios_complementario_path(@estudios_complementario),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
